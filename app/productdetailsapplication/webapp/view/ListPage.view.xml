<mvc:View controllerName="com.productdetailsapplication.productdetailsapplication.controller.ListPage"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
     <Page id="page" title="{i18n>title}">
        <List id="listItems"  itemPress="onListItemPress"  items="{/Products}">
   

        <StandardListItem type="Navigation"
        title="{/ID}"
        icon="sap-icon://begin"
        />
    

     </List>
    </Page>
</mvc:View>
